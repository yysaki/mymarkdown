{
  "name": "mymarkdown",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "yysaki <yysaki@yysaki.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "vue": "^2.5.3",
    "vuetify": "^1.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "jest": "^25.4.0",
    "marked": "^0.8.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^8.0.2",
    "shitajicss": "^5.1.0",
    "style-loader": "^1.1.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.3",
    "vuex": "^3.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
